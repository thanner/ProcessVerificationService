<?xml version="1.0" encoding="UTF-8"?>
<specificationSet
	xmlns="http://www.citi.qut.edu.au/yawl"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	version="Beta 4"
	xsi:schemaLocation="http://www.citi.qut.edu.au/yawl d:/yawl/schema/YAWL_SchemaBeta4.xsd"
>
	<specification uri="Bpmnnet">
		<name>Bpmnnet</name>
		<documentation>Bpmnnet</documentation>
		<metaData/>
		<decomposition
			id="decompositionStart"
			xsi:type="WebServiceGatewayFactsType"
		>
		</decomposition>
		<decomposition
			id="root"
			isRootNet="true"
			xsi:type="NetFactsType"
		>
			<processControlElements>
				<inputCondition
					id="Node0"
				>
					<flowsInto>
						<nextElementRef id="Node1"/>
					</flowsInto>
				</inputCondition>
				<task
					id="Node1"
				>
					<name>inTask_root</name>
					<documentation>inTask_root</documentation>
					<flowsInto>
						<nextElementRef id="Node5"/>
						<predicate>true()</predicate>
					</flowsInto>
					<flowsInto>
						<nextElementRef id="Node8"/>
						<predicate>true()</predicate>
					</flowsInto>
					<flowsInto>
						<nextElementRef id="Node6"/>
						<predicate>true()</predicate>
						<isDefaultFlow/>
					</flowsInto>
					<join code="xor"/>
					<split code="or"/>
					<decomposesTo id="decompositionStart"/>
				</task>
				<task
					id="Node3"
				>
					<name>outTask_root</name>
					<documentation>outTask_root</documentation>
					<flowsInto>
						<nextElementRef id="Node2"/>
					</flowsInto>
					<join code="or"/>
					<split code="and"/>
				</task>
				<task
					id="Node4"
				>
					<name>Do activity 1
</name>
					<documentation>Do activity 1
</documentation>
					<flowsInto>
						<nextElementRef id="Node9"/>
					</flowsInto>
					<join code="xor"/>
					<split code="and"/>
				</task>
				<condition
					id="Node5"
				>
					<name></name>
					<flowsInto>
						<nextElementRef id="Node3"/>
					</flowsInto>
				</condition>
				<condition
					id="Node6"
				>
					<name></name>
					<flowsInto>
						<nextElementRef id="Node4"/>
					</flowsInto>
				</condition>
				<condition
					id="Node7"
				>
					<name></name>
					<flowsInto>
						<nextElementRef id="Node3"/>
					</flowsInto>
				</condition>
				<task
					id="Node8"
				>
					<name>Do activity 3
</name>
					<documentation>Do activity 3
</documentation>
					<flowsInto>
						<nextElementRef id="Node7"/>
					</flowsInto>
					<join code="xor"/>
					<split code="and"/>
				</task>
				<task
					id="Node9"
				>
					<name>Do activity 2
</name>
					<documentation>Do activity 2
</documentation>
					<flowsInto>
						<nextElementRef id="Node3"/>
					</flowsInto>
					<join code="xor"/>
					<split code="and"/>
				</task>
				<outputCondition id="Node2"/>
			</processControlElements>
		</decomposition>
	</specification>
</specificationSet>
