<?xml version="1.0" encoding="UTF-8"?>
<specificationSet
	xmlns="http://www.citi.qut.edu.au/yawl"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	version="Beta 4"
	xsi:schemaLocation="http://www.citi.qut.edu.au/yawl d:/yawl/schema/YAWL_SchemaBeta4.xsd"
>
	<specification uri="Bpmnnet">
		<name>Bpmnnet</name>
		<documentation>Bpmnnet</documentation>
		<metaData/>
		<decomposition
			id="decompositionStart"
			xsi:type="WebServiceGatewayFactsType"
		>
		</decomposition>
		<decomposition
			id="root"
			isRootNet="true"
			xsi:type="NetFactsType"
		>
			<processControlElements>
				<inputCondition
					id="Node0"
				>
					<flowsInto>
						<nextElementRef id="Node1"/>
					</flowsInto>
				</inputCondition>
				<task
					id="Node1"
				>
					<name>inTask_root</name>
					<documentation>inTask_root</documentation>
					<flowsInto>
						<nextElementRef id="Node12"/>
					</flowsInto>
					<join code="xor"/>
					<split code="and"/>
					<decomposesTo id="decompositionStart"/>
				</task>
				<task
					id="Node3"
				>
					<name>outTask_root</name>
					<documentation>outTask_root</documentation>
					<flowsInto>
						<nextElementRef id="Node2"/>
					</flowsInto>
					<join code="xor"/>
					<split code="and"/>
				</task>
				<task
					id="Node4"
				>
					<name></name>
					<documentation></documentation>
					<flowsInto>
						<nextElementRef id="Node7"/>
					</flowsInto>
					<join code="and"/>
					<split code="and"/>
				</task>
				<condition
					id="Node5"
				>
					<name></name>
					<flowsInto>
						<nextElementRef id="Node3"/>
					</flowsInto>
				</condition>
				<task
					id="Node6"
				>
					<name></name>
					<documentation></documentation>
					<flowsInto>
						<nextElementRef id="Node15"/>
						<predicate ordering="0">true()</predicate>
					</flowsInto>
					<flowsInto>
						<nextElementRef id="Node13"/>
						<isDefaultFlow/>
					</flowsInto>
					<join code="xor"/>
					<split code="xor"/>
				</task>
				<task
					id="Node7"
				>
					<name></name>
					<documentation></documentation>
					<flowsInto>
						<nextElementRef id="Node9"/>
					</flowsInto>
					<join code="xor"/>
					<split code="and"/>
				</task>
				<task
					id="Node8"
				>
					<name>Replace part
</name>
					<documentation>Replace part
</documentation>
					<flowsInto>
						<nextElementRef id="Node4"/>
					</flowsInto>
					<join code="xor"/>
					<split code="and"/>
				</task>
				<task
					id="Node9"
				>
					<name>Fill the repair form
</name>
					<documentation>Fill the repair form
</documentation>
					<flowsInto>
						<nextElementRef id="Node5"/>
					</flowsInto>
					<join code="xor"/>
					<split code="and"/>
				</task>
				<task
					id="Node10"
				>
					<name>Make evaluation
</name>
					<documentation>Make evaluation
</documentation>
					<flowsInto>
						<nextElementRef id="Node6"/>
					</flowsInto>
					<join code="xor"/>
					<split code="and"/>
				</task>
				<task
					id="Node11"
				>
					<name>Fill the part replacement form
</name>
					<documentation>Fill the part replacement form
</documentation>
					<flowsInto>
						<nextElementRef id="Node4"/>
					</flowsInto>
					<join code="xor"/>
					<split code="and"/>
				</task>
				<condition
					id="Node12"
				>
					<name></name>
					<flowsInto>
						<nextElementRef id="Node10"/>
					</flowsInto>
				</condition>
				<task
					id="Node13"
				>
					<name>Make a backup</name>
					<documentation>Make a backup</documentation>
					<flowsInto>
						<nextElementRef id="Node14"/>
					</flowsInto>
					<join code="xor"/>
					<split code="and"/>
				</task>
				<task
					id="Node14"
				>
					<name>Format computer</name>
					<documentation>Format computer</documentation>
					<flowsInto>
						<nextElementRef id="Node7"/>
					</flowsInto>
					<join code="xor"/>
					<split code="and"/>
				</task>
				<task
					id="Node15"
				>
					<name></name>
					<documentation></documentation>
					<flowsInto>
						<nextElementRef id="Node8"/>
					</flowsInto>
					<flowsInto>
						<nextElementRef id="Node11"/>
					</flowsInto>
					<join code="xor"/>
					<split code="and"/>
				</task>
				<outputCondition id="Node2"/>
			</processControlElements>
		</decomposition>
	</specification>
</specificationSet>
